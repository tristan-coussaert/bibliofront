    "s3": "^4.4.0",
    "source-map-explorer": "^1.5.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles/sass/*.scss -o src/styles/css/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles/sass/style.min.scss -o src/styles/css/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-ts build",
    "eject": "react-scripts-ts eject",
    "start-mockapi": "node src/api/server",
    "analyze": "source-map-explorer build/static/js/main.*",
    "local": " REACT_APP_API_ENV='local' npm start",
    "rokin": " REACT_APP_API_ENV='rokin' npm start",
    "sakshi": " REACT_APP_API_ENV='sakshi' npm start",
    "sharmila": " REACT_APP_API_ENV='sharmila' npm start",
    "dev": " REACT_APP_API_ENV='dev' npm run build",
    "stage": " REACT_APP_API_ENV='stage' npm run build",
    "prod": " REACT_APP_API_ENV='prod' npm run build",
    "s3-upload": "node tools/s3-upload.js",
    "publish:webapp": "npm run dev && node tools/s3-upload.js"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.50",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.21",
    "@types/react-dom": "^16.0.2",
    "@types/react-loadable": "^5.3.0",
    "@types/react-redux": "^5.0.12",
    "@types/react-router-dom": "^4.2.0",
    "@types/redux-logger": "^3.0.5",
    "json-server": "^0.12.1",
    "node-sass": "^4.7.2",
    "rebuild": "^0.1.2"
  }
}
